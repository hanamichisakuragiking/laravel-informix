{
    "name": "hanamichisakuragiking/laravel-informix",
    "description": "IBM Informix database driver for Laravel 12+",
    "keywords": ["laravel", "informix", "database", "pdo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason",
            "email": "jason@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Hanamichisakuragiking\\LaravelInformix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hanamichisakuragiking\\LaravelInformix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hanamichisakuragiking\\LaravelInformix\\InformixServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
